# if
workflow iftest [
	in [
		input number;
	]
	out [
		output number;
		whut string optional;
	]
	wfomap [[
		$o{output} = $v{counter};
		$o{whut} = $v{thing} if $v{thing};
	]]
	do [
		if [[ $a{input} < 10 ]] then [
			call add [
				imap [[	$i{counter} = $a{input}; ]]
				omap [[
					$v{thing} = 'less than 10';
					$v{counter} = $o{counter};
				]]
			]
		] elsif [[ $a{input} > 100 ]] then [
			call add [
				imap [[	$i{counter} = $a{input}; ]]
				omap [[
					$v{thing} = 'greater than 100';
					$v{counter} = $o{counter};
				]]
			]
		] else [
			call add [
				imap [[	$i{counter} = $a{input}; ]]
				omap [[ $v{counter} = $o{counter}; ]]
			]
		]
	]
]
