# childjob
workflow "childjoberrortest" [
	in [
		"input" number;
	]
	out [
		"output" string;
	]
	wfomap [[
		$o{output} = $v{output};
	]]
	do [
		try [
			call raise_errortest2 [
				imap [[	$i{in} = "$a{input}" ]]
				omap [[ $v{output} = $o{out} * 2; ]]
			]
		] catch [
			eval [[
				if ($v{error}->{class} == 'childerror') {
					$v{output} = 'got childerror';
				} else {
					$v{output} = 'got other error';
				}
			]]
		]
	]
]
